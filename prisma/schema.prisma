generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Registro {
  id        Int      @id @default(autoincrement())
  titulo    String
  foto      String
  area      String
  maquina   String
  tipo      String
  createdAt DateTime @default(now())
}


model Area {
  id Int @id @default(autoincrement())
  nombre String
}

model Maquina {
  id Int @id @default(autoincrement())
  nombre String
  area String
}
